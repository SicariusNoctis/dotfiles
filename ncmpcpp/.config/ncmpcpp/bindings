# Navigation
def_key "j"
  scroll_down
def_key "k"
  scroll_up
def_key "h"
  jump_to_parent_directory
def_key "l"
  enter_directory
def_key "ctrl-b"
  page_up
def_key "ctrl-f"
  page_down
def_key "ctrl-u"
  page_up
def_key "ctrl-d"
  page_down
def_key "g"
  move_home
def_key "G"
  move_end
def_key "f"
  find
def_key "f"
  find_item_forward
def_key "n"
  next_found_item
def_key "N"
  previous_found_item
def_key "space"
  jump_to_playing_song

# Selection
def_key "v"
  select_item
def_key "V"
  select_item
  select_range
def_key "ctrl-v"
  remove_selection
def_key "J"
  select_item
  scroll_down
def_key "K"
  select_item
  scroll_up

# Move
def_key "ctrl-j"
    move_selected_items_down
def_key "ctrl-k"
    move_selected_items_up
def_key "m"
    move_selected_items_to
    move_selected_items_down
def_key "p"
    move_selected_items_to
    move_selected_items_down
def_key "P"
    move_selected_items_to

# Playback
def_key "x"
    pause

# Playlist
def_key "d"
  delete_playlist_items
def_key "d"
  delete_stored_playlist

# Seeking
def_key "left"
  seek_backward
def_key "right"
  seek_forward
def_key "F"
  jump_to_position_in_song

# Miscellaneous
def_key "L"
  show_lyrics
def_key "f4"
  apply_filter
def_key "b"
  set_volume
