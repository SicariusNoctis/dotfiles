def_key "d"
  delete_playlist_items
def_key "d"
  delete_stored_playlist

def_key "ctrl-u"
  page_up
def_key "ctrl-d"
  page_down

def_key "L"
  show_lyrics

def_key "J"
  jump_to_position_in_song

def_key "g"
  move_home
def_key "G"
  move_end

